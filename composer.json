{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "akaunting/laravel-setting": "^1.2",
        "anhskohbo/no-captcha": "^3.4",
        "artesaos/seotools": "^1.2",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-snappy": "^1.0",
        "bulkgate/sms": "^2.1",
        "ckeditor/ckeditor": "^4.16",
        "guzzlehttp/guzzle": "^7.8",
        "hashids/hashids": "^4.1",
        "kkomelin/laravel-translatable-string-exporter": "^1.17",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/slack-notification-channel": "^2.5",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.1",
        "laravelcollective/html": "^6.3",
        "maatwebsite/excel": "^3.1",
        "mcamara/laravel-localization": "^2.0",
        "mediaburst/clockworksms": "^2.0",
        "paragonie/constant_time_encoding": "^2.6",
        "paragonie/recaptcha": "^2.1",
        "pragmarx/countries": "^0.7.2",
        "pragmarx/google2fa-laravel": "^2.0",
        "scyllaly/hcaptcha": "^4.4",
        "spatie/laravel-database-mail-templates": "^3.5",
        "spatie/laravel-permission": "^5.7",
        "spatie/laravel-sluggable": "^3.5",
        "spatie/laravel-translatable": "^6.7",
        "stevebauman/location": "^6.5",
        "symfony/http-client": "^6.3",
        "symfony/mailgun-mailer": "^6.3",
        "tecnickcom/tcpdf": "^6.5",
        "vonage/client": "^4.0",
        "whichbrowser/parser": "^2.1",
        "yajra/laravel-datatables": "^9.0",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
